generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
 url      = env("DATABASE_URL")
}

model Cliente {
  id       String @id @default(uuid())
  nome     String
  telefone String
  email    String @unique
  endereco String
  idade Int?   // Nova coluna adicionada (opcional, pois tem "?")
  createdAt DateTime @default(now())
}
